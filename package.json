{
  "name": "@openlab/portals",
  "version": "0.1.0",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build:lib": "tsc",
    "serve:example": "./example/server.js",
    "format": "prettier --write '**/*.{js,ts,tsx,json,css,md}'",
    "preversion": "npm run -s build:lib"
  },
  "workspaces": [
    "example"
  ],
  "files": [
    "dist/*"
  ],
  "exports": {
    ".": {
      "types":"./dist/lib.d.ts",
      "import":"./dist/lib.js"
    },
    "./*.js": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js"
    }
  },
  "devDependencies": {
    "@types/node": "^16.18.3",
    "@types/ws": "^8.5.3",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4",
    "ws": "^8.10.0"
  },
  "optionalDependencies": {
    "ws": "^8.10.0"
  },
  "lint-staged": {
    "*.{js,json,css,md,ts,tsx}": [
      "prettier --write"
    ]
  }
}
